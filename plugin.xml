<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Bluemix Template Creation"
            id="bluemix">
      </category>
      <wizard
            name="Bluemix Template Creation "
            icon="icons/sample.gif"
            category="bluemix"
            class="com.bluemix.ui.BluemixTemapleCreationWizard"
            id="com.bluemix.ui.BluemixTemapleCreationWizard">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            id="bluemix.contribution1">
         <menu
               label="Bluemix Actions"
               path="additions"
               id="bluemix.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="bluemix.rest.UpdateAction"
               enablesFor="?"
               id="bluemix.updateAction"
               label="Update"
               menubarPath="bluemix.menu1/group1">
         </action>
         <action
               class="bluemix.rest.CreateAction"
               enablesFor="?"
               id="bluemix.newAction"
               label="Create"
               menubarPath="bluemix.menu1/group1">
         </action>
         
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="Bluemix Server Configurations"
            class="bluemix.ui.BluemixPreferencePage"
            id="bluemix.ui.BluemixPreferencePage">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="bluemix.ui.PreferenceInitializer">
      </initializer>
   </extension>
   

</plugin>
